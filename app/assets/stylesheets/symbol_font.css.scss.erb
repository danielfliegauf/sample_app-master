
.button1 {
  -moz-box-shadow:inset 0px 0px 0px 0px #000000;
  -webkit-box-shadow:inset 0px 0px 0px 0px #000000;
  box-shadow:inset 0px 0px 0px 0px #000000;
  background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #363636), color-stop(1, #262626) );
  background:-moz-linear-gradient( center top, #363636 5%, #262626 100% );
  filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#363636', endColorstr='#262626');
  background-color:#363636;
  -moz-border-radius:6px;
  -webkit-border-radius:6px;
  border-radius:6px;
  border:1px solid #615f61;
  display:inline-block;
  margin: 10px;
  color:#ffffff;
  font-family:arial;
  font-size:15px;
  font-weight:bold;
  padding:6px 24px;
  text-decoration:none;
  text-shadow:1px 1px 0px #a8a5a8;
}.classname:hover {
  background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #262626), color-stop(1, #363636) );
  background:-moz-linear-gradient( center top, #262626 5%, #363636 100% );
  filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#262626', endColorstr='#363636');
  background-color:#262626;
}.classname:active {
  position:relative;
  top:1px;
}




@font-face {
