<!DOCTYPE html>
<html>
<head>
	<title><%= full_title(yield(:title)) %></title>
	<%= stylesheet_link_tag "custom", media: "all" %>
	<%= stylesheet_link_tag "jquery-ui-1.9.0.custom", media: "all" %>
	<%= stylesheet_link_tag "timepicker", media: "all" %>
	<%= stylesheet_link_tag "symbol_font" %>
 	<%= javascript_include_tag "application" %>
	<script type="text/javascript" src="https://maps-api-ssl.google.com/maps/api/js?v=3&sensor=true"></script>
 	<%= csrf_meta_tags %>
	<%= render 'layouts/shim' %>
</head>
<body>
	<noscript>
	    <style type="text/css">
	        .pagecontainer {display:none;}
	    </style>
	    <div class="noscriptmsg">
		    <%= t("nojs") %> 
	    </div>
	</noscript>
	<div class="pagecontainer">
		<div id="fb-root"></div>
		<%= render 'layouts/header' %>
		<div class="container">
			<% flash.each do |key, value| %>
				<div class="alert alert-<%= key %>"><%= value %> </div>
			<% end %>
			<%= yield %>
			<%= render 'layouts/footer' %>
			<%= debug(params) if Rails.env.development? %>
		</div>
	</div>
</body>
</html>